<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="400.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
   <children>
    <TitledPane animated="false" collapsible="false" text="Window">
      <content>
        <AnchorPane>
               <children>
                  <GridPane layoutX="7.0" layoutY="29.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" percentWidth="25.0" />
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Label text="Type:" />
                        <Label text="Size:" GridPane.rowIndex="1" />
                        <ComboBox fx:id="windowTypeComboBox" prefWidth="150.0" GridPane.columnIndex="1" />
                        <ComboBox fx:id="windowSizeComboBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                     </children>
                  </GridPane>
                  <CheckBox fx:id="padToHeightCheckBox" layoutX="14.0" layoutY="71.0" mnemonicParsing="false" text="pad to chart height" />
               </children>
            </AnchorPane>
      </content>
    </TitledPane>
    <TitledPane animated="false" collapsible="false" text="Max frequency">
      <content>
        <AnchorPane>
               <children>
                  <Slider fx:id="frequencySlider" layoutX="14.0" layoutY="14.0" majorTickUnit="20.0" minorTickCount="1" showTickLabels="true" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" />
               </children>
            </AnchorPane>
      </content>
    </TitledPane>
      <TitledPane animated="false" collapsible="false" text="Palette">
        <content>
          <AnchorPane>
               <children>
                  <ComboBox fx:id="paletteTypeComboBox" layoutX="24.0" layoutY="25.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" />
               </children>
            </AnchorPane>
        </content>
      </TitledPane>
   </children>
</VBox>
